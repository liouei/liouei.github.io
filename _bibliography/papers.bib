@article{Wei:20,
	author = {Li-Wei Wei and Kanioar Karan and Benno Willke},
	journal = {Appl. Opt.},
	keywords = {Heterodyne detection; Magnetic fields; Optical design; Optical testing; Particle physics; Photons},
	number = {28},
	pages = {8839--8847},
	publisher = {Optica Publishing Group},
	title = {Optics mounting and alignment for the central optical bench of the dual cavity enhanced light-shining-through-a-wall experiment ALPS II},
	volume = {59},
	month = {Oct},
	year = {2020},
	url = {https://opg.optica.org/ao/abstract.cfm?URI=ao-59-28-8839},
	doi = {10.1364/AO.401346},
	abstract = {Any Light Particle Search II (ALPS II) is a light-shining-through-a-wall experiment seeking axion-like particles. ALPS II will feature two 120 m long linear optical cavities that are separated by a wall and support the same photon mode. The central optical bench at the core of the experiment will be equipped with a light-tight shutter and two planar mirrors for the cavities. We show that the mounting concept for ALPS II provides sufficient angular stability and verify that a simple autocollimator assisted alignment procedure for crucial components of the ALPS II optical cavities can lead to the required overlap of the cavity eigenmodes. Furthermore, we show that mounted quadrant photodiodes added to the optical bench can have sufficient stability to maintain this overlap even without a clear line of sight between the two optical cavities.},
  selected = {true}
}


